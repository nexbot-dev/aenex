    "deployGuildCommands": "node dist/core/deployGuildCommands.js",
    "build": "tsc",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "common-tags": "^1.8.2",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.2",
    "sequelize": "^6.21.6"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "typescript": "^4.8.3"
  },
  "imports": {
    "#commands/*": [
      "./dist/commands/*.js"
    ],
    "#core/*": [
      "./dist/core/*.js"
    ],
    "#events/*": [
      "./dist/events/*.js"
    ],
    "#libs/*": [
      "./dist/libs/*.js"
    ],
    "#root/*": [
      "./dist/*.js"
