{
  "name": "aenex",
  "version": "0.1.1",
  "description": "Aenex",
  "keywords": [
    "aenex",
    "nexbot",
    "discord bot"
  ],
  "author": "AenJojo",
  "license": "GPL-3.0",
  "main": "dist/app.js",
  "private": true,
  "type": "module",
  "scripts": {
