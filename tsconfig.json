{
  "compilerOptions": {
    "target": "es2022",
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "#commands/*": ["./src/commands/*"],
      "#core/*": ["./src/core/*"],
      "#events/*": ["./src/events/*"],
      "#libs/*": ["src/libs/*"],
      "#root/*": ["./src/*"]
    },
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "useUnknownInCatchVariables": true,
    "skipLibCheck": true,
  }
}